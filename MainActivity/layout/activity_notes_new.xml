<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawerLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<!-- â•â•â• MAIN CONTENT â•â•â• -->
<FrameLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home_gradient_bg">

    <!-- Main scrollable content -->
    <androidx.core.widget.NestedScrollView
        android:id="@+id/mainScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:scrollbars="none"
        android:clipToPadding="false"
        android:paddingBottom="88dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!-- â•â•â• HEADER â•â•â• -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingHorizontal="16dp"
                android:paddingTop="12dp"
                android:paddingBottom="4dp">

                <!-- Top Row: Hamburger + Title + Compose -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical">

                    <ImageButton
                        android:id="@+id/btnDrawerToggle"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:background="?attr/selectableItemBackgroundBorderless"
                        android:src="@android:drawable/ic_menu_view"
                        android:contentDescription="Menu"
                        android:tint="#FFFFFF"
                        android:scaleType="centerInside" />

                    <TextView
                        android:id="@+id/tvNotesTitle"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Notes"
                        android:textColor="#FFFFFF"
                        android:textSize="28sp"
                        android:textStyle="bold"
                        android:fontFamily="sans-serif-medium"
                        android:layout_marginStart="8dp" />

                    <!-- View toggle (grid/list) â€” kept in header right side -->
                    <ImageButton
                        android:id="@+id/btnViewToggle"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:background="?attr/selectableItemBackgroundBorderless"
                        android:src="@android:drawable/ic_dialog_dialer"
                        android:contentDescription="Toggle View"
                        android:tint="#94A3B8"
                        android:scaleType="centerInside" />

                    <!-- More/overflow menu -->
                    <ImageButton
                        android:id="@+id/btnMoreMenu"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:background="?attr/selectableItemBackgroundBorderless"
                        android:src="@android:drawable/ic_menu_more"
                        android:contentDescription="More"
                        android:tint="#94A3B8"
                        android:scaleType="centerInside" />
                </LinearLayout>

                <!-- Date + Count pill row -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="2dp">

                    <TextView
                        android:id="@+id/tvDateLine"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Wednesday, Feb 25"
                        android:textColor="#F59E0B"
                        android:textSize="13sp"
                        android:fontFamily="sans-serif" />

                    <TextView
                        android:id="@+id/tvNoteCount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0 notes"
                        android:textColor="#64748B"
                        android:textSize="11sp"
                        android:fontFamily="sans-serif-medium"
                        android:background="@drawable/notes_count_pill_bg"
                        android:paddingHorizontal="10dp"
                        android:paddingVertical="3dp" />
                </LinearLayout>
            </LinearLayout>

            <!-- â•â•â• SEARCH BAR â•â•â• -->
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="16dp"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="4dp">

                <EditText
                    android:id="@+id/etSearchNotes"
                    android:layout_width="match_parent"
                    android:layout_height="48dp"
                    android:hint="Search notes, tags..."
                    android:textColorHint="#475569"
                    android:textColor="#E2E8F0"
                    android:textSize="14sp"
                    android:background="@drawable/notes_search_bg"
                    android:paddingStart="44dp"
                    android:paddingEnd="44dp"
                    android:singleLine="true"
                    android:imeOptions="actionSearch"
                    android:inputType="text" />

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="start|center_vertical"
                    android:layout_marginStart="14dp"
                    android:src="@android:drawable/ic_menu_search"
                    android:tint="#475569" />

                <ImageButton
                    android:id="@+id/btnVoiceSearch"
                    android:layout_width="36dp"
                    android:layout_height="36dp"
                    android:layout_gravity="end|center_vertical"
                    android:layout_marginEnd="8dp"
                    android:background="?attr/selectableItemBackgroundBorderless"
                    android:src="@android:drawable/ic_btn_speak_now"
                    android:contentDescription="Voice Search"
                    android:tint="#475569"
                    android:scaleType="centerInside" />
            </FrameLayout>

            <!-- Recent Searches (hidden by default) -->
            <LinearLayout
                android:id="@+id/recentSearchesContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingHorizontal="20dp"
                android:visibility="gone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Recent Searches"
                    android:textColor="#64748B"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:layout_marginBottom="6dp" />

                <LinearLayout
                    android:id="@+id/recentSearchesList"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" />
            </LinearLayout>

            <!-- â•â•â• FILTER CHIPS ROW â•â•â• -->
            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scrollbars="none"
                android:layout_marginTop="8dp"
                android:paddingVertical="4dp"
                android:clipToPadding="false">

                <LinearLayout
                    android:id="@+id/chipContainer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingHorizontal="16dp"
                    android:gravity="center_vertical" />
            </HorizontalScrollView>

            <!-- â•â•â• RECENTLY VIEWED STRIP â•â•â• -->
            <LinearLayout
                android:id="@+id/recentlyViewedSection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="16dp"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="20dp"
                    android:layout_marginBottom="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="ðŸ• Recently Viewed"
                        android:textColor="#94A3B8"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        android:fontFamily="sans-serif-medium" />
                </LinearLayout>

                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scrollbars="none"
                    android:clipToPadding="false">

                    <LinearLayout
                        android:id="@+id/recentlyViewedContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingHorizontal="16dp" />
                </HorizontalScrollView>
            </LinearLayout>

            <!-- â•â•â• TO-DO LISTS SECTION â•â•â• -->
            <LinearLayout
                android:id="@+id/todoListsSection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:visibility="visible">

                <!-- Header row -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="20dp"
                    android:paddingBottom="8dp">

                    <TextView
                        android:id="@+id/tvTodoHeader"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="To-Do Lists"
                        android:textColor="#FFFFFF"
                        android:textSize="18sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tvTodoSummary"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0 active"
                        android:textColor="#94A3B8"
                        android:textSize="12sp"
                        android:layout_marginEnd="8dp" />

                    <TextView
                        android:id="@+id/btnViewAllTodos"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="View All"
                        android:textColor="#60A5FA"
                        android:textSize="13sp"
                        android:paddingVertical="4dp"
                        android:paddingHorizontal="8dp" />
                </LinearLayout>

                <!-- Horizontal scrollable list cards -->
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/todoListsRecycler"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:clipToPadding="false"
                    android:paddingHorizontal="16dp" />

            </LinearLayout>

            <!-- â•â•â• FOLDER STRIP â•â•â• -->            <LinearLayout
                android:id="@+id/folderStripSection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="16dp"
                android:visibility="gone">

                <!-- Header row -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:paddingHorizontal="20dp"
                    android:layout_marginBottom="10dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Folders"
                        android:textColor="#F1F5F9"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:fontFamily="sans-serif-medium" />

                    <TextView
                        android:id="@+id/btnViewAllFolders"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="View All"
                        android:textColor="#F59E0B"
                        android:textSize="13sp"
                        android:paddingHorizontal="8dp"
                        android:paddingVertical="4dp" />
                </LinearLayout>

                <!-- Horizontal folder cards scroll -->
                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scrollbars="none"
                    android:clipToPadding="false">

                    <LinearLayout
                        android:id="@+id/folderCardsContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingHorizontal="16dp"
                        android:paddingBottom="4dp" />
                </HorizontalScrollView>
            </LinearLayout>

            <!-- â•â•â• PINNED SECTION â•â•â• -->
            <LinearLayout
                android:id="@+id/pinnedSection"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone"
                android:layout_marginTop="12dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ðŸ“Œ  Pinned"
                    android:textColor="#F59E0B"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:fontFamily="sans-serif-medium"
                    android:layout_marginHorizontal="20dp"
                    android:layout_marginBottom="8dp" />

                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scrollbars="none"
                    android:clipToPadding="false">

                    <LinearLayout
                        android:id="@+id/pinnedNotesContainer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingHorizontal="16dp" />
                </HorizontalScrollView>
            </LinearLayout>

            <!-- â•â•â• OTHERS / ALL NOTES HEADER â•â•â• -->
            <TextView
                android:id="@+id/tvOthersHeader"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="All Notes"
                android:textColor="#94A3B8"
                android:textSize="14sp"
                android:textStyle="bold"
                android:fontFamily="sans-serif-medium"
                android:layout_marginHorizontal="20dp"
                android:layout_marginTop="16dp"
                android:layout_marginBottom="8dp"
                android:visibility="gone" />

            <!-- â•â•â• NOTES GRID (Staggered) â•â•â• -->
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/notesRecyclerView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:nestedScrollingEnabled="false"
                android:clipToPadding="false"
                android:paddingHorizontal="12dp"
                android:paddingBottom="16dp" />

            <!-- â•â•â• EMPTY STATE â•â•â• -->
            <LinearLayout
                android:id="@+id/emptyStateContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingTop="60dp"
                android:paddingHorizontal="40dp"
                android:visibility="gone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ðŸ““"
                    android:textSize="72sp"
                    android:layout_marginBottom="20dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="No notes yet"
                    android:textColor="#E2E8F0"
                    android:textSize="22sp"
                    android:textStyle="bold"
                    android:fontFamily="sans-serif-medium"
                    android:gravity="center" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tap + to write your first thought.\nCapture ideas, tasks, and everything between."
                    android:textColor="#64748B"
                    android:textSize="14sp"
                    android:gravity="center"
                    android:lineSpacingExtra="4dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="24dp" />

                <TextView
                    android:id="@+id/btnEmptyCreateNote"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Create your first note"
                    android:textColor="#FFFFFF"
                    android:textSize="14sp"
                    android:background="@drawable/notes_fab_bg"
                    android:paddingHorizontal="24dp"
                    android:paddingVertical="12dp" />
            </LinearLayout>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

    <!-- â•â•â• MULTI-SELECT BOTTOM BAR â•â•â• -->
    <LinearLayout
        android:id="@+id/multiSelectBar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="bottom"
        android:background="@drawable/notes_bottom_bar_bg"
        android:orientation="horizontal"
        android:gravity="center"
        android:paddingHorizontal="8dp"
        android:visibility="gone"
        android:elevation="8dp">

        <LinearLayout
            android:id="@+id/btnMultiPin"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:background="?attr/selectableItemBackground">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="ðŸ“Œ" android:textSize="20sp" />
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="Pin" android:textColor="#94A3B8" android:textSize="11sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnMultiColor"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:background="?attr/selectableItemBackground">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="ðŸŽ¨" android:textSize="20sp" />
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="Color" android:textColor="#94A3B8" android:textSize="11sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnMultiCategory"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:background="?attr/selectableItemBackground">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="ðŸ“‚" android:textSize="20sp" />
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="Category" android:textColor="#94A3B8" android:textSize="11sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnMultiArchive"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:background="?attr/selectableItemBackground">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="ðŸ“¦" android:textSize="20sp" />
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="Archive" android:textColor="#94A3B8" android:textSize="11sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btnMultiDelete"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            android:background="?attr/selectableItemBackground">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="ðŸ—‘ï¸" android:textSize="20sp" />
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:text="Delete" android:textColor="#94A3B8" android:textSize="11sp" />
        </LinearLayout>
    </LinearLayout>

    <!-- â•â•â• SPEED DIAL OVERLAY â•â•â• -->
    <View
        android:id="@+id/speedDialOverlay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/notes_speed_dial_overlay_bg"
        android:visibility="gone"
        android:clickable="true" />

    <!-- â•â•â• SPEED DIAL ITEMS â•â•â• -->
    <LinearLayout
        android:id="@+id/speedDialItem4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="240dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:visibility="gone">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Image Note"
            android:textColor="#FFFFFF"
            android:textSize="13sp"
            android:background="@drawable/notes_chip_bg"
            android:paddingHorizontal="12dp"
            android:paddingVertical="6dp"
            android:layout_marginEnd="10dp" />
        <FrameLayout
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:background="@drawable/notes_speed_dial_teal_bg">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_gravity="center" android:text="ðŸ–¼ï¸" android:textSize="20sp" />
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/speedDialItem3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="190dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:visibility="gone">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Voice Note"
            android:textColor="#FFFFFF"
            android:textSize="13sp"
            android:background="@drawable/notes_chip_bg"
            android:paddingHorizontal="12dp"
            android:paddingVertical="6dp"
            android:layout_marginEnd="10dp" />
        <FrameLayout
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:background="@drawable/notes_speed_dial_purple_bg">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_gravity="center" android:text="ðŸŽ¤" android:textSize="20sp" />
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/speedDialItem2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="140dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:visibility="gone">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Checklist"
            android:textColor="#FFFFFF"
            android:textSize="13sp"
            android:background="@drawable/notes_chip_bg"
            android:paddingHorizontal="12dp"
            android:paddingVertical="6dp"
            android:layout_marginEnd="10dp" />
        <FrameLayout
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:background="@drawable/notes_speed_dial_blue_bg">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_gravity="center" android:text="â˜‘ï¸" android:textSize="20sp" />
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/speedDialItem1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="90dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:visibility="gone">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Note"
            android:textColor="#FFFFFF"
            android:textSize="13sp"
            android:background="@drawable/notes_chip_bg"
            android:paddingHorizontal="12dp"
            android:paddingVertical="6dp"
            android:layout_marginEnd="10dp" />
        <FrameLayout
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:background="@drawable/notes_speed_dial_bg">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                android:layout_gravity="center" android:text="ðŸ“" android:textSize="20sp" />
        </FrameLayout>
    </LinearLayout>

    <!-- â•â•â• FAB â•â•â• -->
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fabAddNote"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="24dp"
        app:backgroundTint="#F59E0B"
        app:tint="#FFFFFF"
        android:src="@android:drawable/ic_input_add"
        android:contentDescription="Add Note"
        app:elevation="6dp" />

</FrameLayout>

<!-- â•â•â• NAV DRAWER â•â•â• -->
<include layout="@layout/nav_drawer_notes" />

</androidx.drawerlayout.widget.DrawerLayout>
